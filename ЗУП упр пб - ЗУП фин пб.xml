			<Алгоритм Имя="ПередВыгрузкойДокументаНайтиПодразделениеФин" ИспользуетсяПриЗагрузке="false">
				<Текст>// - нвПодразделениеУпр
// - нвОрганизацияФин, нвОрганизацияФинСотрудника
// - нвСотрудник
нвПодразделениеФин = Неопределено; // результат вычисления
нвНаименованиеФИН = "";

// реализация соответствий упр-фин "многие ко многим"
Запросы.соотвПодразделениеФинСотрудника.УстановитьПараметр("Сотрудник", нвСотрудник);
Выборка = Запросы.соотвПодразделениеФинСотрудника.Выполнить().Выбрать();
Пока Выборка.Следующий() Цикл
	Если ЗначениеЗаполнено(Выборка.ПодразделениеФин) Тогда
		нвПодразделениеФин = Выборка.ПодразделениеФин;
		Прервать;
	КонецЕсли;
КонецЦикла;

// если ничего не найдено, значит новый механизм не задействован - считаем по-старому

// вычисление подразделения для Опта
Если НЕ ЗначениеЗаполнено(нвПодразделениеФин) И Параметры.ЭтаИБ_Опт Тогда // определяем подразделение УПР по свойству
	
	ТаблицаПодразделений = Новый ТаблицаЗначений;
	ТаблицаПодразделений.Колонки.Добавить("Подразделение", Новый ОписаниеТипов("СправочникСсылка.ПодразделенияОрганизаций"));
	ТаблицаПодразделений.Колонки.Добавить("Приоритет", Новый ОписаниеТипов("Число", Новый КвалификаторыЧисла(2, 0, ДопустимыйЗнак.Неотрицательный)));
	
	ТекПодразделение = нвПодразделениеУпр;
	ТекПриоритет = 1;
	Пока ЗначениеЗаполнено(ТекПодразделение) Цикл
		НоваяСтрока = ТаблицаПодразделений.Добавить();
		НоваяСтрока.Подразделение = ТекПодразделение;
		НоваяСтрока.Приоритет = ТекПриоритет;
		ТекПриоритет = ТекПриоритет + 1;
		ТекПодразделение = ТекПодразделение.Родитель;
	КонецЦикла;
	
	Запросы.ПоискПодразделенияПоПравиламОпта.УстановитьПараметр("Подразделения", ТаблицаПодразделений);
	Выборка = Запросы.ПоискПодразделенияПоПравиламОпта.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		нвПодразделениеУпр = Выборка.Подразделение;
		нвНаименованиеФИН = Выборка.НаименованиеФИН;
	КонецЕсли;
	
КонецЕсли;

Если НЕ ЗначениеЗаполнено(нвПодразделениеФин) И ЗначениеЗаполнено(нвОрганизацияФин) Тогда
	Если нвОбменДаннымиПолныеПраваПовтИсп.ПолучитьЗначениеКонстанты("нвИспользуетсяНастройкаСоотествийПодразделений") Тогда
		стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствия(нвПодразделениеУпр, нвОрганизацияФин, ТекущаяДата());
	Иначе
		стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствияПоИмени(нвПодразделениеУпр, нвОрганизацияФин);
	КонецЕсли;
	нвПодразделениеФин = стрПодр.Подразделение;
КонецЕсли;
Если НЕ ЗначениеЗаполнено(нвПодразделениеФин) И ЗначениеЗаполнено(нвОрганизацияФинСотрудника) Тогда
	Если нвОбменДаннымиПолныеПраваПовтИсп.ПолучитьЗначениеКонстанты("нвИспользуетсяНастройкаСоотествийПодразделений") Тогда
		стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствия(нвПодразделениеУпр, нвОрганизацияФинСотрудника, ТекущаяДата());
	Иначе
		стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствияПоИмени(нвПодразделениеУпр, нвОрганизацияФинСотрудника);
	КонецЕсли;
	нвПодразделениеФин = стрПодр.Подразделение;
КонецЕсли;</Текст>
				<Параметры>нвПодразделениеУпр, нвОрганизацияФин, нвОрганизацияФинСотрудника, нвСотрудник</Параметры>
			</Алгоритм>
		</Группа>
	</Алгоритмы>
	<Запросы>
		<Группа Имя="Документы">
			<Запрос Имя="ТаблицаПриемаНаРаботуДляНачислений" ИспользуетсяПриЗагрузке="false">
				<Текст>ВЫБРАТЬ
	СтрокиДокумента.Сотрудник,
	СтрокиДокумента.ФизЛицо,
	СтрокиДокумента.ПодразделениеОрганизации КАК ПодразделениеУпр,
	СтрокиДокумента.нвПодразделениеФИН КАК ПодразделениеФин,
	ВЫБОР
		КОГДА СтрокиДокумента.нвДолжностьФИН = ЗНАЧЕНИЕ(Справочник.ДолжностиОрганизаций.ПустаяСсылка)
			ТОГДА СтрокиДокумента.Должность
		ИНАЧЕ СтрокиДокумента.нвДолжностьФИН
	КОНЕЦ КАК Должность,
	СтрокиДокумента.нвОрганизацияФИН КАК ОрганизацияФин,
	СтрокиДокумента.ДатаПриема
ИЗ
	Документ.ПриемНаРаботуВОрганизацию.РаботникиОрганизации КАК СтрокиДокумента
ГДЕ
	СтрокиДокумента.Ссылка = &amp;Ссылка
	И НЕ СтрокиДокумента.Сторно
	И СтрокиДокумента.нвОрганизацияФИН &lt;&gt; ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
	И СтрокиДокумента.нвСтатус В (ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.Постоянная), ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.ВременнаяПочасовая))</Текст>
			</Запрос>
			<Запрос Имя="ФинНачисленияДокумента" ИспользуетсяПриЗагрузке="false">
				<Текст>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОсновныеНачисленияОрганизаций.Ссылка КАК ВидРасчета
ПОМЕСТИТЬ ОсновныеВР
ИЗ
	ПланВидовРасчета.ОсновныеНачисленияОрганизаций КАК ОсновныеНачисленияОрганизаций
ГДЕ
	ОсновныеНачисленияОрганизаций.ЗачетОтработанногоВремени
	И НЕ ОсновныеНачисленияОрганизаций.ТребуетВводаТарифногоРазряда
	И ОсновныеНачисленияОрганизаций.СпособРасчета В (ЗНАЧЕНИЕ(Перечисление.СпособыРасчетаОплатыТруда.ПоМесячнойТарифнойСтавкеПоДням), ЗНАЧЕНИЕ(Перечисление.СпособыРасчетаОплатыТруда.ПоМесячнойТарифнойСтавкеПоЧасам), ЗНАЧЕНИЕ(Перечисление.СпособыРасчетаОплатыТруда.ПоДневнойТарифнойСтавке), ЗНАЧЕНИЕ(Перечисление.СпособыРасчетаОплатыТруда.ПоЧасовойТарифнойСтавке))
	И НЕ ОсновныеНачисленияОрганизаций.ПометкаУдаления

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	нвИспользуемыеВидыНачислений.ВидНачисления
ИЗ
	РегистрСведений.нвИспользуемыеВидыНачислений КАК нвИспользуемыеВидыНачислений
ГДЕ
	нвИспользуемыеВидыНачислений.КатегорияУчета = ЗНАЧЕНИЕ(Перечисление.нвКатегорииУчетаВидовНачислений.ОсновныеНачисления)
	И НЕ нвИспользуемыеВидыНачислений.ФИН
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Организации.Ссылка КАК Организация,
	ВЫБОР
		КОГДА Организации.РайонныйКоэффициент &gt; 1
			ТОГДА Организации.РайонныйКоэффициент - 1
		ИНАЧЕ 0
	КОНЕЦ * 100 КАК ПроцентРК
ПОМЕСТИТЬ РКОрганизации
ИЗ
	Справочник.Организации КАК Организации
ГДЕ
	Организации.Ссылка = &amp;ОрганизацияФин
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	&amp;ОрганизацияФин КАК Организация,
	Работники.Ссылка,
	Работники.Сотрудник,
	Работники.ФизЛицо,
	Работники.Должность.Грейд КАК Грейд,
	Работники.Должность,
	Работники.ПодразделениеОрганизации
ПОМЕСТИТЬ СотрудникиДокумента
ИЗ
	Документ.ПриемНаРаботуВОрганизацию.РаботникиОрганизации КАК Работники
ГДЕ
	Работники.Ссылка = &amp;ТекущийДокумент
	И Работники.нвСтатус В (ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.Постоянная), ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.ВременнаяПочасовая))
	И НЕ Работники.Сторно
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОценкиКомпетенцийРаботниковСрезПоследних.ФизЛицо,
	МАКСИМУМ(ОценкиКомпетенцийРаботниковСрезПоследних.Оценка) КАК Оценка
ПОМЕСТИТЬ ОценкиКомпетенцийСотрудников
ИЗ
	РегистрСведений.ОценкиКомпетенцийРаботников.СрезПоследних(
			&amp;ДатаАктуальности,
			ФизЛицо В
					(ВЫБРАТЬ
						СотрудникиДокумента.Сотрудник
					ИЗ
						СотрудникиДокумента КАК СотрудникиДокумента)
				И Компетенция В
					(ВЫБРАТЬ
						КомпетенцииРаботников.Ссылка
					ИЗ
						Справочник.КомпетенцииРаботников КАК КомпетенцииРаботников
					ГДЕ
						КомпетенцииРаботников.ШкалаОценок = ЗНАЧЕНИЕ(Справочник.ШкалыОценокКомпетенций.нвПодуровеньДолжности)
						И НЕ КомпетенцииРаботников.ПометкаУдаления)) КАК ОценкиКомпетенцийРаботниковСрезПоследних

СГРУППИРОВАТЬ ПО
	ОценкиКомпетенцийРаботниковСрезПоследних.ФизЛицо
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОценкиКомпетенцийРаботниковСрезПоследних.ФизЛицо,
	МАКСИМУМ(ОценкиКомпетенцийРаботниковСрезПоследних.Оценка) КАК Оценка
ПОМЕСТИТЬ ОценкиКомпетенцийФизлиц
ИЗ
	РегистрСведений.ОценкиКомпетенцийРаботников.СрезПоследних(
			&amp;ДатаАктуальности,
			ФизЛицо В
					(ВЫБРАТЬ
						СотрудникиДокумента.ФизЛицо
					ИЗ
						СотрудникиДокумента КАК СотрудникиДокумента)
				И Компетенция В
					(ВЫБРАТЬ
						КомпетенцииРаботников.Ссылка
					ИЗ
						Справочник.КомпетенцииРаботников КАК КомпетенцииРаботников
					ГДЕ
						КомпетенцииРаботников.ШкалаОценок = ЗНАЧЕНИЕ(Справочник.ШкалыОценокКомпетенций.нвПодуровеньДолжности)
						И НЕ КомпетенцииРаботников.ПометкаУдаления)) КАК ОценкиКомпетенцийРаботниковСрезПоследних

СГРУППИРОВАТЬ ПО
	ОценкиКомпетенцийРаботниковСрезПоследних.ФизЛицо
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СотрудникиДокумента.Организация,
	СотрудникиДокумента.Ссылка,
	СотрудникиДокумента.Сотрудник,
	СотрудникиДокумента.ФизЛицо,
	СотрудникиДокумента.Грейд,
	ВЫБОР
		КОГДА НЕ ОценкиКомпетенцийСотрудников.Оценка ЕСТЬ NULL 
			ТОГДА ОценкиКомпетенцийСотрудников.Оценка
		КОГДА НЕ ОценкиКомпетенцийФизлиц.Оценка ЕСТЬ NULL 
			ТОГДА ОценкиКомпетенцийФизлиц.Оценка
		ИНАЧЕ МинимальныеПодуровни.МинимальныйПодуровень
	КОНЕЦ КАК Подуровень,
	СотрудникиДокумента.Должность,
	СотрудникиДокумента.ПодразделениеОрганизации
ПОМЕСТИТЬ СотрудникиДокументаОценки
ИЗ
	СотрудникиДокумента КАК СотрудникиДокумента
		ЛЕВОЕ СОЕДИНЕНИЕ ОценкиКомпетенцийСотрудников КАК ОценкиКомпетенцийСотрудников
		ПО СотрудникиДокумента.Сотрудник = ОценкиКомпетенцийСотрудников.ФизЛицо
		ЛЕВОЕ СОЕДИНЕНИЕ ОценкиКомпетенцийФизлиц КАК ОценкиКомпетенцийФизлиц
		ПО СотрудникиДокумента.ФизЛицо = ОценкиКомпетенцийФизлиц.ФизЛицо
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			СоставШкалГрейдовСрезПоследних.Уровень КАК Грейд,
			СоставШкалГрейдовСрезПоследних.Подуровень КАК МинимальныйПодуровень
		ИЗ
			РегистрСведений.нвСоставШкалУровнейГрейда.СрезПоследних(&amp;ДатаАктуальности, Организация В (&amp;ОрганизацияУпр)) КАК СоставШкалГрейдовСрезПоследних
		ГДЕ
			СоставШкалГрейдовСрезПоследних.Действует
			И (СоставШкалГрейдовСрезПоследних.Уровень, СоставШкалГрейдовСрезПоследних.Подуровень.ПриоритетОценки) В
					(ВЫБРАТЬ
						СоставШкалГрейдовСрезПоследних.Уровень КАК Грейд,
						МИНИМУМ(СоставШкалГрейдовСрезПоследних.Подуровень.ПриоритетОценки) КАК МинимальнаяОценка
					ИЗ
						РегистрСведений.нвСоставШкалУровнейГрейда.СрезПоследних(&amp;ДатаАктуальности, Организация В (&amp;ОрганизацияУпр)) КАК СоставШкалГрейдовСрезПоследних
					ГДЕ
						СоставШкалГрейдовСрезПоследних.Действует
					СГРУППИРОВАТЬ ПО
								СоставШкалГрейдовСрезПоследних.Уровень)) КАК МинимальныеПодуровни
		ПО СотрудникиДокумента.Грейд = МинимальныеПодуровни.Грейд
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СотрудникиДокументаОценки.Сотрудник,
	МАКСИМУМ(ВЫБОР
			КОГДА ОграниченияВыплатСрезПоследних.Ставка ЕСТЬ NULL 
				ТОГДА ШтатноеРасписаниеОрганизацийСрезПоследних.МинимальнаяТарифнаяСтавка
			ИНАЧЕ ОграниченияВыплатСрезПоследних.Ставка
		КОНЕЦ) КАК ОкладФин
ПОМЕСТИТЬ ОкладыФинСотрудников
ИЗ
	СотрудникиДокументаОценки КАК СотрудникиДокументаОценки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ШтатноеРасписаниеОрганизаций.СрезПоследних(&amp;ДатаАктуальности, ПодразделениеОрганизации.Владелец = &amp;ОрганизацияФин) КАК ШтатноеРасписаниеОрганизацийСрезПоследних
		ПО СотрудникиДокументаОценки.Должность = ШтатноеРасписаниеОрганизацийСрезПоследних.Должность
			И СотрудникиДокументаОценки.ПодразделениеОрганизации.Наименование = ШтатноеРасписаниеОрганизацийСрезПоследних.ПодразделениеОрганизации.Наименование
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.нвОграниченияВыплатЛинейномуПерсоналу.СрезПоследних(
				&amp;ДатаАктуальности,
				Организация = &amp;ОрганизацияУпр
					И ВидНачисления = &amp;ВРОкладФИН) КАК ОграниченияВыплатСрезПоследних
		ПО СотрудникиДокументаОценки.ПодразделениеОрганизации.Владелец = ОграниченияВыплатСрезПоследних.Организация
			И СотрудникиДокументаОценки.Грейд = ОграниченияВыплатСрезПоследних.Уровень
			И СотрудникиДокументаОценки.Подуровень = ОграниченияВыплатСрезПоследних.Подуровень

СГРУППИРОВАТЬ ПО
	СотрудникиДокументаОценки.Сотрудник
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВложенныйЗапрос.Ссылка,
	ВложенныйЗапрос.НомерСтроки,
	ВложенныйЗапрос.Сотрудник,
	ВложенныйЗапрос.ФизЛицо,
	ВложенныйЗапрос.ВидРасчета,
	МАКСИМУМ(ВложенныйЗапрос.Показатель1) Как Показатель1,
	МАКСИМУМ(ВложенныйЗапрос.Валюта1) Как Валюта1,
	МАКСИМУМ(ВложенныйЗапрос.Показатель2) Как Показатель2,
	МАКСИМУМ(ВложенныйЗапрос.Валюта2) Как Валюта2,
	МАКСИМУМ(ВложенныйЗапрос.Показатель3) Как Показатель3,
	МАКСИМУМ(ВложенныйЗапрос.Валюта3) Как Валюта3,
	МАКСИМУМ(ВложенныйЗапрос.Показатель4) Как Показатель4,
	МАКСИМУМ(ВложенныйЗапрос.Валюта4) Как Валюта4,
	МАКСИМУМ(ВложенныйЗапрос.Показатель5) Как Показатель5,
	МАКСИМУМ(ВложенныйЗапрос.Валюта5) Как Валюта5,
	МАКСИМУМ(ВложенныйЗапрос.Показатель6) Как Показатель6,
	МАКСИМУМ(ВложенныйЗапрос.Валюта6) Как Валюта6,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд1) Как ТарифныйРазряд1,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд2) Как ТарифныйРазряд2,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд3) Как ТарифныйРазряд3,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд4) Как ТарифныйРазряд4,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд5) Как ТарифныйРазряд5,
	МАКСИМУМ(ВложенныйЗапрос.ТарифныйРазряд6) Как ТарифныйРазряд6,
	ВложенныйЗапрос.Сторно
ИЗ
(ВЫБРАТЬ 
	Начисления.Ссылка,
	NULL Как НомерСтроки,
	Начисления.Сотрудник,
	Начисления.ФизЛицо,
	ВЫБОР
		КОГДА Начисления.ВидРасчета В
				(ВЫБРАТЬ
					ОсновныеВР.ВидРасчета
				ИЗ
					ОсновныеВР КАК ОсновныеВР)
			ТОГДА ОсновныеВРФИН.ВидНачисления
		ИНАЧЕ Начисления.ВидРасчета
	КОНЕЦ КАК ВидРасчета,
	ВЫБОР
		КОГДА Начисления.ВидРасчета В
				(ВЫБРАТЬ
					ОсновныеВР.ВидРасчета
				ИЗ
					ОсновныеВР КАК ОсновныеВР)
			ТОГДА ОкладыФинСотрудников.ОкладФин
		ИНАЧЕ Начисления.Показатель1
	КОНЕЦ * Работники.ЗанимаемыхСтавок КАК Показатель1,
	Начисления.Валюта1,
	Начисления.Показатель2,
	Начисления.Валюта2,
	Начисления.Показатель3,
	Начисления.Валюта3,
	Начисления.Показатель4,
	Начисления.Валюта4,
	Начисления.Показатель5,
	Начисления.Валюта5,
	Начисления.Показатель6,
	Начисления.Валюта6,
	Начисления.ТарифныйРазряд1,
	Начисления.ТарифныйРазряд2,
	Начисления.ТарифныйРазряд3,
	Начисления.ТарифныйРазряд4,
	Начисления.ТарифныйРазряд5,
	Начисления.ТарифныйРазряд6,
	Начисления.Сторно
ИЗ
	Документ.ПриемНаРаботуВОрганизацию.ОсновныеНачисления КАК Начисления
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ПриемНаРаботуВОрганизацию.РаботникиОрганизации КАК Работники
			ЛЕВОЕ СОЕДИНЕНИЕ ОкладыФинСотрудников КАК ОкладыФинСотрудников
			ПО Работники.Сотрудник = ОкладыФинСотрудников.Сотрудник
		ПО Начисления.Сотрудник = Работники.Сотрудник
			И Начисления.Ссылка = Работники.Ссылка,
	(ВЫБРАТЬ
		ИспользуемыеВидыНачислений.ВидНачисления КАК ВидНачисления
	ИЗ
		РегистрСведений.нвИспользуемыеВидыНачислений КАК ИспользуемыеВидыНачислений
	ГДЕ
		ИспользуемыеВидыНачислений.КатегорияУчета = ЗНАЧЕНИЕ(Перечисление.нвКатегорииУчетаВидовНачислений.ОсновныеНачисления)
		И ИспользуемыеВидыНачислений.ФИН) КАК ОсновныеВРФИН
ГДЕ
	Начисления.Ссылка = &amp;ТекущийДокумент
	И Работники.нвСтатус В (ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.Постоянная), ЗНАЧЕНИЕ(Перечисление.нвСтатусыШтатнойЕдиницы.ВременнаяПочасовая))
	И (Начисления.ВидРасчета.Предопределенный
			ИЛИ Начисления.ВидРасчета В
				(ВЫБРАТЬ
					ОсновныеВР.ВидРасчета
				ИЗ
					ОсновныеВР КАК ОсновныеВР))
	И НЕ Работники.Сторно
	И НЕ Начисления.Сторно

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СотрудникиДокумента.Ссылка,
	NULL,
	СотрудникиДокумента.Сотрудник,
	СотрудникиДокумента.ФизЛицо,
