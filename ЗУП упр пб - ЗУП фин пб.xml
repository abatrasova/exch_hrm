//		И нвОбщегоНазначения.ЕстьРеквизитДокумента("Упр", нвФизЛицо.Метаданные()) Тогда
//		
//		Если нвФизЛицо.Упр И ЗначениеЗаполнено(нвФизЛицо.ФизЛицоФин) Тогда
//			Значение = нвФизЛицо.ФизЛицоФин;
//		КонецЕсли;
//		
//	КонецЕсли;
//КонецЕсли;</Текст>
			<Параметры>нвФизЛицо</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ПВД_ПередОбработкой" ИспользуетсяПриЗагрузке="false">
			<Текст>Отказ = Параметры.ЭтаИБ_Компанейский;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ПВД_Табель_ПередОбработкой" ИспользуетсяПриЗагрузке="false">
			<Текст>Док = Документы.ТабельУчетаРабочегоВремениОрганизации.СоздатьДокумент();
ТаблицаТЧОтработанноеВремя = Док.ОтработанноеВремя.ВыгрузитьКолонки();

ТаблицаТабель = ТаблицаТЧОтработанноеВремя.Скопировать();
ТаблицаТабель.Колонки.Добавить("Дата");
ТаблицаТабель.Колонки.Добавить("Ссылка");
ТаблицаТабель.Колонки.Добавить("ПометкаУдаления");
ТаблицаТабель.Колонки.Добавить("Проведен");
ТаблицаТабель.Колонки.Добавить("Организация");
ТаблицаТабель.Колонки.Добавить("ПериодРегистрации");
ТаблицаТабель.Колонки.Добавить("Ответственный");
//ТаблицаТабель.Колонки.Добавить("Комментарий");
ТаблицаТабель.Колонки.Добавить("СпособВводаДанных");
ТаблицаТабель.Колонки.Добавить("СпособУказанияПериода");
ТаблицаТабель.Колонки.Добавить("ДатаНачалаПериода");
ТаблицаТабель.Колонки.Добавить("ДатаОкончанияПериода");
ТаблицаТабель.Колонки.Добавить("ИДДокумента");
ТаблицаТабель.Колонки.Добавить("ПодразделениеОрганизации");
ТаблицаТабель.Колонки.Добавить("ДанныеФИНПодготовлены");
ТаблицаТабель.Колонки.Добавить("нвВыгружатьВФИН");

ЭтотУзел = ПланыОбмена.нвПоОрганизации.ЭтотУзел();
ВыборкаУзлы = ПланыОбмена.нвПоОрганизации.Выбрать();
Пока ВыборкаУзлы.Следующий() Цикл
	
	ТекущийУзел = ВыборкаУзлы.Ссылка;
	Если ТекущийУзел &lt;&gt; УзелДляОбмена Тогда //ТекущийУзел = ЭтотУзел ИЛИ ТекущийУзел.ПометкаУдаления Тогда
		Продолжить;
	КонецЕсли;
	
	// обход по объектам, зарегистрированным для выгрузки
	ВыборкаИзменения = ПланыОбмена.ВыбратьИзменения(ТекущийУзел,ТекущийУзел.НомерОтправленного+1);
	Пока ВыборкаИзменения.Следующий() Цикл
		Объект = ВыборкаИзменения.Получить();
		Если ТипЗнч(Объект) = Тип("ДокументОбъект.ТабельУчетаРабочегоВремениОрганизации")
			ИЛИ ТипЗнч(Объект) = Тип("ДокументСсылка.ТабельУчетаРабочегоВремениОрганизации") Тогда
			
			Для Каждого СтрокаТЧТабеля Из Объект.ОтработанноеВремя Цикл
				
				Сотр = СтрокаТЧТабеля.Сотрудник;
				Статус = Сотр.нвСтатус;
				Запросы.ТекущийСтатусСотрудника.УстановитьПараметр("ДатаСреза", КонецДня(Объект.ДатаОкончанияПериода));
				Запросы.ТекущийСтатусСотрудника.УстановитьПараметр("Сотрудник", Сотр);
				Выборка = Запросы.ТекущийСтатусСотрудника.Выполнить().Выбрать();
				Если Выборка.Следующий() Тогда
					Статус = Выборка.Статус;
				КонецЕсли;
				Если Статус &lt;&gt; Перечисления.нвСтатусыШтатнойЕдиницы.Постоянная
					И Статус &lt;&gt; Перечисления.нвСтатусыШтатнойЕдиницы.ВременнаяПочасовая Тогда
					Продолжить;
				КонецЕсли;
				
				ОрганизацияФИН = Сотр.нвОрганизацияФИН;
				Если НЕ ЗначениеЗаполнено(ОрганизацияФИН) Тогда
					Продолжить;
				КонецЕсли;
				
				НоваяСтрока = ТаблицаТабель.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаТЧТабеля);
				НоваяСтрока.Дата = Объект.Дата;
				НоваяСтрока.Ссылка = Новый УникальныйИдентификатор();//Объект.Ссылка;
				НоваяСтрока.ПометкаУдаления = Объект.ПометкаУдаления;
				НоваяСтрока.Проведен = Объект.Проведен;
				НоваяСтрока.Организация = ОрганизацияФИН;
				Если нвОбменДаннымиПолныеПраваПовтИсп.ПолучитьЗначениеКонстанты("нвИспользуетсяНастройкаСоотествийПодразделений") Тогда
					стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствия(Объект.ПодразделениеОрганизации, ОрганизацияФИН, Объект.Дата);
				Иначе
					стрПодр = нвСтруктураКомпании.ПолучитьПодразделениеСоответствияПоИмени(Объект.ПодразделениеОрганизации, ОрганизацияФИН);
				КонецЕсли;
				НоваяСтрока.ПодразделениеОрганизации = стрПодр.Подразделение;
				НоваяСтрока.ПериодРегистрации = Объект.ПериодРегистрации;
				НоваяСтрока.Ответственный = Объект.Ответственный;
				НоваяСтрока.Комментарий = ""+Объект.Комментарий;
				НоваяСтрока.СпособВводаДанных = Объект.СпособВводаДанных;
				НоваяСтрока.СпособУказанияПериода = Объект.СпособУказанияПериода;
				НоваяСтрока.ДатаНачалаПериода = Объект.ДатаНачалаПериода;
				НоваяСтрока.ДатаОкончанияПериода = Объект.ДатаОкончанияПериода;
				НоваяСтрока.ИДДокумента = СокрЛП(Объект.Ссылка.УникальныйИдентификатор())+СокрЛП(ОрганизацияФИН.УникальныйИдентификатор());
				НоваяСтрока.Сотрудник = Сотр;
				НоваяСтрока.ФизЛицо = Сотр.Физлицо;
				НоваяСтрока.ДанныеФИНПодготовлены = Объект.ДанныеФИНПодготовлены;
				НоваяСтрока.нвВыгружатьВФИН = Объект.нвВыгружатьВФИН;
				
			КонецЦикла;
			
		КонецЕсли;
	КонецЦикла;
	
КонецЦикла;

ТаблицаТабель.Сортировать("ИДДокумента");

// формирование таблицы конвертации документа
ВыборкаДанных = Новый ТаблицаЗначений;
ВыборкаДанных.Колонки.Добавить("Дата");
ВыборкаДанных.Колонки.Добавить("Ссылка");
ВыборкаДанных.Колонки.Добавить("ПометкаУдаления");
ВыборкаДанных.Колонки.Добавить("Проведен");
ВыборкаДанных.Колонки.Добавить("Организация");
ВыборкаДанных.Колонки.Добавить("ПериодРегистрации");
ВыборкаДанных.Колонки.Добавить("Ответственный");
ВыборкаДанных.Колонки.Добавить("Комментарий");
ВыборкаДанных.Колонки.Добавить("СпособВводаДанных");
ВыборкаДанных.Колонки.Добавить("СпособУказанияПериода");
ВыборкаДанных.Колонки.Добавить("ДатаНачалаПериода");
ВыборкаДанных.Колонки.Добавить("ДатаОкончанияПериода");
ВыборкаДанных.Колонки.Добавить("ИДДокумента");
ВыборкаДанных.Колонки.Добавить("ОтработанноеВремя"); //ТЧ
ВыборкаДанных.Колонки.Добавить("ПодразделениеОрганизации");
ВыборкаДанных.Колонки.Добавить("ДанныеФИНПодготовлены");
ВыборкаДанных.Колонки.Добавить("нвВыгружатьВФИН");

ТекКлюч = "";
СтрокаДокумент = Неопределено;
ТабТЧ = Неопределено;
Для Каждого СтрокаТаблицы Из ТаблицаТабель Цикл
	
	Если ТекКлюч &lt;&gt; СтрокаТаблицы.ИДДокумента Тогда
		
		Если СтрокаДокумент &lt;&gt; Неопределено Тогда
			СтрокаДокумент.ОтработанноеВремя = ТабТЧ.Скопировать();
			ВыгрузитьПоПравилу(СтрокаДокумент,,,,"ТабельУчетаРабочегоВремениОрганизации");
		КонецЕсли;
		ТекКлюч = СтрокаТаблицы.ИДДокумента;
		ТабТЧ = ТаблицаТЧОтработанноеВремя.Скопировать();
		ТабТЧ.Очистить();
		
		СтрокаДокумент = ВыборкаДанных.Добавить();
		ЗаполнитьЗначенияСвойств(СтрокаДокумент,СтрокаТаблицы);
		
	КонецЕсли;
	
	СтрокаТЧ = ТабТЧ.Добавить();
	ЗаполнитьЗначенияСвойств(СтрокаТЧ,СтрокаТаблицы);
	
КонецЦикла;

Если СтрокаДокумент &lt;&gt; Неопределено Тогда
	СтрокаДокумент.ОтработанноеВремя = ТабТЧ.Скопировать();
	ВыгрузитьПоПравилу(СтрокаДокумент,,,,"ТабельУчетаРабочегоВремениОрганизации");
КонецЕсли;

Отказ = Истина;</Текст>
