		<Алгоритм Имя="ПКО_Документы_ПослеЗагрузки" ИспользуетсяПриЗагрузке="true">
			<Текст>// - нвВидДок
// Результат: Отказ

пОрганизация = Объект.Организация;

//Нети (Батрасова А. 20.02.2016, наряд 0000011156
