		Запросы.ФизлицоСотрудникРаботает.УстановитьПараметр("ПоВсемОрганизациям", Ложь);
		Выборка = Запросы.ФизлицоСотрудникРаботает.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			Если НЕ (Выборка.Работает &gt; 0) Тогда
				Отказ = Истина;
			КонецЕсли;
		Иначе
			Отказ = Истина;
		КонецЕсли;
		
	ИначеЕсли нвВидДок = "РегистрацияРазовыхУдержанийРаботниковОрганизаций" Тогда
		
		НачПериода = НачалоМесяца(Объект.ПериодРегистрации);
		КонПериода = КонецМесяца(Объект.ПериодРегистрации);
		
		ТЧДокумента = Объект.Удержания;
		ИмяРеквизитаСотрудник = "Физлицо";
		ЭтоФизлицо = Истина;
		пПоВсемОрганизациям = Истина;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		
	ИначеЕсли нвВидДок = "РегистрацияРазовыхНачисленийРаботниковОрганизаций" Тогда
		
		НачПериода = НачалоМесяца(Объект.ПериодРегистрации);
		КонПериода = КонецМесяца(Объект.ПериодРегистрации);
		
		ТЧДокумента = Объект.ОсновныеНачисления;
		ИмяРеквизитаСотрудник = "Сотрудник";
		ЭтоФизлицо = Ложь;
		пПоВсемОрганизациям = Истина;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		
		Если НЕ Отказ Тогда
			ТЧДокумента = Объект.ДополнительныеНачисления;
			Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		КонецЕсли;
		
	ИначеЕсли нвВидДок = "НевыходыВОрганизациях" Тогда
		
		НачПериода = НачалоМесяца(Объект.ПериодРегистрации);
		КонПериода = КонецМесяца(Объект.ПериодРегистрации);
		
		ТЧДокумента = Объект.Начисления;
		ИмяРеквизитаСотрудник = "Сотрудник";
		ЭтоФизлицо = Ложь;
		пПоВсемОрганизациям = Ложь;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		
	ИначеЕсли нвВидДок = "ЗарплатаКВыплатеОрганизаций" Тогда
		
		НачПериода = НачалоМесяца(Объект.ПериодРегистрации);
		КонПериода = КонецМесяца(Объект.ПериодРегистрации);
		
		ТЧДокумента = Объект.Зарплата;
		ИмяРеквизитаСотрудник = "Сотрудник";
		ЭтоФизлицо = Ложь;
		пПоВсемОрганизациям = Ложь;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		ИмяРеквизитаСотрудник = "Физлицо";
		ЭтоФизлицо = Истина;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		
		ЗаписьЖурналаРегистрации("Обмен данными.УПР -&gt; ФИН", УровеньЖурналаРегистрации.Информация, , ,
			"ЗарплатаКВыплате.ПослеЗагрузки: Объект = " + Объект + "; Строк: " + ТЧДокумента.Количество());
		
	ИначеЕсли нвВидДок = "РасходныйКассовыйОрдер" Тогда
		
		НачПериода = НачалоМесяца(Объект.Дата);
		КонПериода = КонецМесяца(Объект.Дата);
		
		ТЧДокумента = Объект.Работники;
		ИмяРеквизитаСотрудник = "Физлицо";
		ЭтоФизлицо = Истина;
		пПоВсемОрганизациям = Истина;
		Выполнить(Алгоритмы.Доп_ОбработкатьТабличнуюЧасть_СотрудникиРаботают);
		
	КонецЕсли;

КонецЕсли;</Текст>
