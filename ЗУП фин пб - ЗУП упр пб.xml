					ПроставлятьМП26 = Истина;	
				КонецЕсли;
				
				Если ПроставлятьМП26 И (КодВР = "МПС" ИЛИ КодВР = "МПР" ИЛИ КодВР = "МП") И ВыгружатьНачислениеДополнительное Тогда
					НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.ДополнительныеНачисленияОрганизаций.НайтиПоКоду("МП26");
				ИначеЕсли (КодВР = "МПС" ИЛИ КодВР = "МПР" ИЛИ КодВР = "МП26") И ВыгружатьНачислениеДополнительное Тогда
				//Если (КодВР = "МПС" ИЛИ КодВР = "МПР") И ВыгружатьНачислениеДополнительное Тогда
				//Нети )Батрасова А. 02.09.2016
					НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.ДополнительныеНачисленияОрганизаций.НайтиПоКоду("МП");
				Иначе
					НоваяСтрокаТЧ.ВидРасчета = ЗаписьРегистра.ВидРасчета;
				КонецЕсли;
			КонецЕсли;
			НоваяСтрокаТЧ.ДатаНачала = пДатаНачала;
			НоваяСтрокаТЧ.ДатаОкончания = пДатаОкончания;
			НоваяСтрокаТЧ.ПодразделениеОрганизации = ЗаписьРегистра.ПодразделениеОрганизации;
			НоваяСтрокаТЧ.Показатель1 = ЗаписьРегистра.Результат;
			НоваяСтрокаТЧ.Результат = ЗаписьРегистра.Результат;
			НоваяСтрокаТЧ.Сотрудник = ЗаписьРегистра.Сотрудник;
			НоваяСтрокаТЧ.Физлицо = ЗаписьРегистра.Физлицо;
			
		КонецЕсли;
		
		// строка разового удержания
		НайденныеСтроки = ВыборкаДанныхУдержания.НайтиСтроки(ОтборПоиска);
		Если НайденныеСтроки.Количество() &gt; 0 Тогда
			НоваяСтрока = НайденныеСтроки[0];
		Иначе
			НоваяСтрока = ВыборкаДанныхУдержания.Добавить();
			НоваяСтрока.Ссылка = ЗаписьРегистра.Регистратор;
			НоваяСтрока.Дата = пДата;
			НоваяСтрока.ПериодРегистрации = НачалоМесяца(ЗаписьРегистра.ПериодРегистрации);
			НоваяСтрока.Организация = пОрганизация;
			НоваяСтрока.Ответственный = пОтветственный;
			НоваяСтрока.ИДДокументаФИН = КлючСтроки;
			Значение = "";
			Источник = ЗаписьРегистра.Регистратор;
			Выполнить(Алгоритмы.ПКС_Комментарий_ПередВыгрузкой);
			НоваяСтрока.Комментарий = Значение + ": " + Строка(ЗаписьРегистра.Регистратор);
			ТЗТЧ = Новый ТаблицаЗначений;
			ТЗТЧ.Колонки.Добавить("Физлицо");
			ТЗТЧ.Колонки.Добавить("ДатаНачала");
			ТЗТЧ.Колонки.Добавить("ДатаОкончания");
			ТЗТЧ.Колонки.Добавить("Показатель1");
			ТЗТЧ.Колонки.Добавить("Результат");
			ТЗТЧ.Колонки.Добавить("ВидРасчета");
			ТЗТЧ.Колонки.Добавить("нвСотрудник"); //Нети (Батрасова А. 20.06.2016, наряд 0000014054
		
			НоваяСтрока.Удержания = ТЗТЧ;
		КонецЕсли;
		
		НоваяСтрокаТЧ = НоваяСтрока.Удержания.Добавить();
		НоваяСтрокаТЧ.Физлицо = ЗаписьРегистра.Физлицо;
		НоваяСтрокаТЧ.ДатаНачала = пДатаНачала;
		НоваяСтрокаТЧ.ДатаОкончания = пДатаОкончания;
		НоваяСтрокаТЧ.Показатель1 = ЗаписьРегистра.Результат;
		НоваяСтрокаТЧ.Результат = ЗаписьРегистра.Результат;
		
		Если Параметры.ЭтоОбменДляЦО Тогда
			Если ТипРегистратора = Тип("ДокументСсылка.НачислениеПоБольничномуЛисту") Тогда
				НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.УдержанияОрганизаций.нвВыплатаНаКартуБЛ;
			ИначеЕсли ТипРегистратора = Тип("ДокументСсылка.НачислениеОтпускаРаботникамОрганизаций") Тогда
				НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.УдержанияОрганизаций.нвВыплатаНаКартуОтпуск;
			Иначе
				НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.УдержанияОрганизаций.нвВыплатаНаКарту;
			КонецЕсли;
		Иначе
			Запросы.УдержаниеУПРпоНачислениюФИН.УстановитьПараметр("ВидНачисления", ЗаписьРегистра.ВидРасчета);
			Выборка = Запросы.УдержаниеУПРпоНачислениюФИН.Выполнить().Выбрать();
			Выборка.Следующий();
			НоваяСтрокаТЧ.ВидРасчета = Выборка.ВидУдержания; //соответствие начислений УПР-ФИН
		КонецЕсли;
		
		//Нети (Батрасова А. 20.06.2016, наряд 0000014054
		НоваяСтрокаТЧ.нвСотрудник = ЗаписьРегистра.Сотрудник;
		//Нети )Батрасова А. 20.06.2016
		
		//Нети (Батрасова А. 12.04.2016, наряд 0000008636
		//проверить необходимость вычета НДФЛ

		Если НоваяСтрокаТЧ.ВидРасчета = ПланыВидовРасчета.УдержанияОрганизаций.нвВыплатаНаКарту Тогда
			Запросы.ДолжностьСотрудникаНаДату.УстановитьПараметр("Период", НоваяСтрокаТЧ.ДатаНачала);
			СотрудникФизлица = ПроцедурыУправленияПерсоналом.ОсновнойСотрудникФизлица(ЗаписьРегистра.Физлицо, ЗаписьРегистра.Организация);
			Запросы.ДолжностьСотрудникаНаДату.УстановитьПараметр("Сотрудник", СотрудникФизлица);
			Запросы.ДолжностьСотрудникаНаДату.УстановитьПараметр("Организация", ЗаписьРегистра.Организация);
			Выборка = Запросы.ДолжностьСотрудникаНаДату.Выполнить().Выбрать();
			Если Выборка.Следующий() Тогда
				нвДолжность = Выборка.Должность; 
			Иначе 
				нвДолжность = СотрудникФизлица.ТекущаяДолжностьОрганизации;
			КонецЕсли;
			Попытка
			Если нвДолжность.Грейд.нвОтниматьНДФЛИзНачислений Тогда
				НоваяСтр = ТЗУдержания.Добавить();
				НоваяСтр.Организация = ЗаписьРегистра.Организация;
				НоваяСтр.ИДДокументаФИН = КлючСтроки;
				НоваяСтр.Физлицо = ЗаписьРегистра.Физлицо;
				НоваяСтр.Результат = ЗаписьРегистра.Результат;  
				НоваяСтр.НомерСтроки = НоваяСтрока.Удержания.Индекс(НоваяСтрокаТЧ);  
				НоваяСтр.ПериодРегистрации = НачалоМесяца(ЗаписьРегистра.ПериодРегистрации);
			КонецЕсли;			
			Исключение
			КонецПопытки;
		КонецЕсли;
		//Нети )Батрасова А. 12.04.2016
		
	КонецЦикла;
	
КонецЦикла;

//Нети (Батрасова А. 12.04.2016, наряд 0000008636
//проверить необходимость вычета НДФЛ
ТЗФизЛицаПериоды = ТЗУдержания.Скопировать();
ТЗФизЛицаПериоды.Свернуть("Физлицо, ПериодРегистрации, Организация");
ТЗФизЛица = ТЗУдержания.Скопировать();
ТЗФизЛица.Свернуть("Физлицо, Организация");
Для Каждого Стр Из ТЗФизЛица Цикл
	СтрокиПоФЛ = ТЗФизЛицаПериоды.НайтиСтроки(Новый Структура("Физлицо, Организация", Стр.Физлицо, Стр.Организация));
	Для Каждого СтрФЛ Из СтрокиПоФЛ Цикл
		Запросы.НДФЛПоФизлицуЗаПериод.УстановитьПараметр("Период", СтрФЛ.ПериодРегистрации);
		Запросы.НДФЛПоФизлицуЗаПериод.УстановитьПараметр("ФизЛицо", СтрФЛ.Физлицо);
		Запросы.НДФЛПоФизлицуЗаПериод.УстановитьПараметр("Организация", СтрФЛ.Организация);
		ВыборкаНДФЛ = Запросы.НДФЛПоФизлицуЗаПериод.Выполнить().Выбрать();
		ВыборкаНДФЛ.Следующий();
		Если ВыборкаНДФЛ.Количество()&gt;0 И ВыборкаНДФЛ.Налог&gt;0 Тогда
			ТаблДляРапределения = ТЗУдержания.Скопировать(Новый Структура("Физлицо, ПериодРегистрации, Организация", СтрФЛ.Физлицо, СтрФЛ.ПериодРегистрации, 
				СтрФЛ.Организация));
			МассивКоэффициентов = ТаблДляРапределения.ВыгрузитьКолонку("Результат");
			МассивСумм = ОбщегоНазначенияЗК.РаспределитьПропорционально(ВыборкаНДФЛ.Налог, МассивКоэффициентов, 0);
			
			инд = 0;
			Для Каждого СтрУдержание Из ТаблДляРапределения Цикл
				СтрокаДокумента = ВыборкаДанныхУдержания.Найти(СтрУдержание.ИДДокументаФИН, "ИДДокументаФИН");
				Если СтрокаДокумента&lt;&gt;Неопределено Тогда
					СтрокаВТЧ = СтрокаДокумента.Удержания.Получить(СтрУдержание.НомерСтроки);
					Если СтрокаВТЧ&lt;&gt;Неопределено Тогда
						СтрокаВТЧ.Результат = ?((СтрокаВТЧ.Результат-МассивСумм[инд])&lt;0, 0, СтрокаВТЧ.Результат-МассивСумм[инд]);
						СтрокаВТЧ.Показатель1 = СтрокаВТЧ.Результат;
					КонецЕсли;
				КонецЕсли;
				инд = инд + 1;
			КонецЦикла;
		КонецЕсли;	
	КонецЦикла;
КонецЦикла;
//Нети )Батрасова А. 12.04.2016</Текст>
