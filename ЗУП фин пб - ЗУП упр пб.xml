			Запросы.ОбменСБухгалтериейПоНовымПравиламНаДату.УстановитьПараметр("Дата", нвДата);
			Запросы.ОбменСБухгалтериейПоНовымПравиламНаДату.УстановитьПараметр("Организация", нвОрганизация);
			Выборка = Запросы.ОбменСБухгалтериейПоНовымПравиламНаДату.Выполнить().Выбрать();
			Если Выборка.Следующий() Тогда
				НовыйУчетВыгрузки = Выборка.ОбменСБухгалтериейПоНовымПравилам;	
			Иначе
				НовыйУчетВыгрузки = Ложь;	
			КонецЕсли;
		КонецЕсли;
        
		//Если нвОбменДаннымиПолныеПраваПовтИсп.ПолучитьЗначениеКонстанты("нвОбменСБухгалтериейПоНовымПравилам") Тогда
		
		Если НовыйУчетВыгрузки Тогда
		//Нети )Батрасова А. 29.02.2016

			ЭтоНелинейщики = Истина;
		Иначе
			нвОтказ = Истина;
		КонецЕсли;
	КонецЕсли;
	
КонецЕсли;</Текст>
			<Параметры>нвДолжность, нвОтказ</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ПКС_Комментарий_ПередВыгрузкой" ИспользуетсяПриЗагрузке="false">
			<Текст>ИсхКомм = "";
Попытка
	ИсхКомм = СокрЛП(Источник.Комментарий);
Исключение
КонецПопытки;
Значение = ИсхКомм + "#Создан " + ТекущаяДата();</Текст>
			<Параметры>Источник</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ПолучитьПодразделениеУпрПередВыгрузкой" ИспользуетсяПриЗагрузке="false">
			<Текст>// - нвПодразделениеФИН
// - нвПериод
// - нвСотрудник
// Результат: Значение

Запросы.соотвПодразделениеУПРСотрудника.УстановитьПараметр("Сотрудник", нвСотрудник);
Запросы.соотвПодразделениеУПРСотрудника.УстановитьПараметр("Период", нвПериод);
РезультатЗапроса = Запросы.соотвПодразделениеУПРСотрудника.Выполнить();
ЕстьСоответствие = Ложь;
Если НЕ РезультатЗапроса.Пустой() Тогда
	Выборка = РезультатЗапроса.Выбрать();
	Если Выборка.Следующий() Тогда
		Значение = Выборка.ПодразделениеОрганизации;
		ЕстьСоответствие = Истина;
	КонецЕсли;
КонецЕсли;

Если НЕ ЕстьСоответствие ИЛИ НЕ ЗначениеЗаполнено(Значение) Тогда
	
	Если Параметры.ЭтаИБ_Опт Тогда
		
		ВремЗначение = Значение;
		Значение = Неопределено;
		нвОрганизацияФИН = нвПодразделениеФИН.Владелец;
		нвПериод = КонецДня(нвПериод);
		Выполнить(Алгоритмы.ОпределитьОрганизациюУПРСотрудника);
		ОрганизацияУпр = Значение;
		Значение = ВремЗначение;
		
		Запросы.ПоискПодразделенияУпрПоПравиламОпта.УстановитьПараметр("ОрганизацияУпр", ОрганизацияУпр);
		Запросы.ПоискПодразделенияУпрПоПравиламОпта.УстановитьПараметр("НаименованиеФин", нвПодразделениеФИН.Наименование);
		Выборка = Запросы.ПоискПодразделенияУпрПоПравиламОпта.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			Значение = Выборка.Подразделение;
		КонецЕсли;
		
	Иначе
		Значение = нвСтруктураКомпании.ПолучитьУпрПодразделение(нвПодразделениеФИН, нвПериод);
	КонецЕсли;
	
КонецЕсли;</Текст>
			<Параметры>нвПодразделениеФИН, нвПериод, нвСотрудник</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ПроверитьВыгрузитьУдалениеУдержаний" ИспользуетсяПриЗагрузке="false">
			<Текст>Запрос = Новый Запрос;
Запрос.УстановитьПараметр("Регистратор", Источник.Ссылка);
Запрос.Текст = 
"ВЫБРАТЬ
|	НДФЛРасчетыСБюджетом.Период
|ИЗ
|	РегистрНакопления.НДФЛРасчетыСБюджетом КАК НДФЛРасчетыСБюджетом
|ГДЕ
|	НДФЛРасчетыСБюджетом.Регистратор = &amp;Регистратор";
РезультатЗапроса = Запрос.Выполнить();

Если (пВидДок = "НачислениеЗарплатыРаботникамОрганизаций" ИЛИ пВидДок = "НачислениеОтпускаРаботникамОрганизаций" ИЛИ пВидДок = "НачислениеПоБольничномуЛисту")
	И (НЕ Источник.Проведен ИЛИ (РезультатЗапроса.Пустой() И Параметры.ЭтаИБ_Опт)) Тогда
	
	ИсходящиеДанные = Новый Структура("Организация, ПериодРегистрации, Дата, ПометкаУдаления, Проведен, ИДДокументаФИН");
	ИсходящиеДанные.ПометкаУдаления = Источник.ПометкаУдаления;
	ИсходящиеДанные.Проведен = Ложь;
	пДатаОкончания = КонецМесяца(Источник.ПериодРегистрации);
	
	пОрганизация = Источник.Организация;
	Если НЕ Параметры.ЭтаИБ_Опт Тогда
		Значение = Неопределено;
		нвСотрудник = Неопределено;
		нвОрганизацияФИН = Источник.Организация;
		нвПериод = пДатаОкончания;
		Выполнить(Алгоритмы.ОпределитьОрганизациюУПРСотрудника);
		пОрганизация = Значение;
	КонецЕсли;
	
	КлючСтроки = СокрЛП(Строка(Источник.Ссылка.УникальныйИдентификатор())) + СокрЛП(Строка(пОрганизация.УникальныйИдентификатор()));
	ИсходящиеДанные.Организация = пОрганизация;
	ИсходящиеДанные.ИДДокументаФИН = КлючСтроки;
	ИсходящиеДанные.Дата = пДатаОкончания;
	ВыгрузитьПоПравилу( , , ИсходящиеДанные, , "РегистрацияРазовыхУдержанийУдаление");
	
	Если пВидДок = "НачислениеЗарплатыРаботникамОрганизаций" Тогда
		ВыгрузитьПоПравилу( , , ИсходящиеДанные, , "РегистрацияРазовыхНачисленийУдаление");
	КонецЕсли;
	
	Отказ = Истина;
	
КонецЕсли;</Текст>
			<Параметры>пВидДок</Параметры>
		</Алгоритм>
