		<Алгоритм Имя="ОпределитьОрганизациюУПРСотрудника" ИспользуетсяПриЗагрузке="false">
			<Текст>// - нвСотрудник
// - нвОрганизацияФИН
// - нвПериод

ЕстьСоответствие = Ложь;

Если ЗначениеЗаполнено(нвСотрудник) Тогда
	
	Запросы.соотвОрганизацияУПРСотрудника.УстановитьПараметр("Сотрудник", нвСотрудник);
	Запросы.соотвОрганизацияУПРСотрудника.УстановитьПараметр("Период", нвПериод);
	РезультатЗапроса = Запросы.соотвОрганизацияУПРСотрудника.Выполнить();
	Если НЕ РезультатЗапроса.Пустой() Тогда
		Выборка = РезультатЗапроса.Выбрать();
		Если Выборка.Следующий() Тогда
			Значение = Выборка.Организация;
			ЕстьСоответствие = Истина;
		КонецЕсли;
	КонецЕсли;
	
КонецЕсли;

Если НЕ ЕстьСоответствие ИЛИ НЕ ЗначениеЗаполнено(Значение) Тогда
	Значение = нвСтруктураКомпании.ПолучитьУпрОрганизацию(нвОрганизацияФИН, нвПериод);
КонецЕсли;</Текст>
			<Параметры>нвСотрудник, нвОрганизацияФИН, нвПериод</Параметры>
		</Алгоритм>
		<Алгоритм Имя="ПВД_РазовыеНачисленияУдержания_ПередОбработкой" ИспользуетсяПриЗагрузке="false">
			<Текст>Если НЕ Параметры.УдержанияВыгружены Тогда
	
	ТаблицаШапкаДокумента = Параметры.ТабЗначРегистрацияУдержаний;
	Для Каждого СтрокаТаблицы Из ТаблицаШапкаДокумента Цикл
		Если СтрокаТаблицы.Удержания.Количество() &gt; 0 Тогда
			//Нети (Батрасова А. 20.06.2016, наряд 0000014054
			//СтрокаТаблицы.Удержания.Свернуть("Физлицо,ДатаНачала,ДатаОкончания,ВидРасчета", "Показатель1,Результат");
			СтрокаТаблицы.Удержания.Свернуть("Физлицо,ДатаНачала,ДатаОкончания,ВидРасчета,нвСотрудник", "Показатель1,Результат");
			//Нети )Батрасова А. 20.06.2016
		КонецЕсли;
		ВыгрузитьПоПравилу(СтрокаТаблицы,,,,"РегистрацияРазовыхУдержанийРаботников");
	КонецЦикла;
	
	ТаблицаШапкаДокумента = Параметры.ТабЗначРегистрацияНачислений;
	Для Каждого СтрокаТаблицы Из ТаблицаШапкаДокумента Цикл
		Если СтрокаТаблицы.ОсновныеНачисления.Количество() &gt; 0 Тогда
			СтрокаТаблицы.ОсновныеНачисления.Свернуть("ВидРасчета,ДатаНачала,ДатаОкончания,ПодразделениеОрганизации,Сотрудник,Физлицо,ДатаНачалаСобытия", "Показатель1,Результат");
		КонецЕсли;
		ВыгрузитьПоПравилу(СтрокаТаблицы,,,,"РегистрацияРазовыхНачисленийРаботников");
	КонецЦикла;
	
	Параметры.УдержанияВыгружены = Истина;
	
КонецЕсли;

Отказ = Истина;</Текст>
		</Алгоритм>
		<Алгоритм Имя="ПКО_Документы_ПередВыгрузкой" ИспользуетсяПриЗагрузке="false">
			<Текст>// - нвСотрудник
//НеЗамещатьОбъектПриЗагрузке = Истина;

Если ТипЗнч(Источник.Ссылка) &lt;&gt; Тип("ДокументСсылка.ЗарплатаКВыплатеОрганизаций") Тогда
	Попытка
		Отказ = НЕ Источник.Проведен;
	Исключение
	КонецПопытки;
КонецЕсли;

Если Параметры.ЭтаИБ_Компания И Источник.Дата &lt; Дата(2015, 4, 1) Тогда
	Отказ = Истина;
КонецЕсли;

Если НЕ Отказ И Параметры.ЭтаИБ_Опт Тогда
	
	Если ЗначениеЗаполнено(нвСотрудник) Тогда
		Запросы.СвойствоНеВыгружатьСотрудника.УстановитьПараметр("Сотрудник", нвСотрудник);
		РезультатЗапроса = Запросы.СвойствоНеВыгружатьСотрудника.Выполнить();
		Если НЕ РезультатЗапроса.Пустой() Тогда
			Выборка = РезультатЗапроса.Выбрать();
			Если Выборка.Следующий() Тогда
				Отказ = Выборка.ЗначениеСвойства;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
	ЕстьПериод = Ложь;
	Попытка
		ЕстьПериод = Источник.Метаданные().Реквизиты.Найти("ПериодРегистрации") &lt;&gt; Неопределено;
	Исключение
	КонецПопытки;
	
	Если ЕстьПериод Тогда
		Если Источник.ПериодРегистрации &lt; Дата(2015, 5, 1) Тогда
			Отказ = Истина;
		КонецЕсли;
	ИначеЕсли Источник.Дата &lt; Дата(2015, 5, 1) Тогда
		Отказ = Истина;
	КонецЕсли;
	
КонецЕсли;

Если НЕ Отказ Тогда
	
	Значение = Неопределено;
	нвОрганизацияФИН = Источник.Организация;
	нвПериод = КонецДня(Источник.Дата);
	Выполнить(Алгоритмы.ОпределитьОрганизациюУПРСотрудника);
	Если ЗначениеЗаполнено(Значение) Тогда
		ВыгрузитьПоПравилу(Значение,,,,"Организации");
	КонецЕсли;
	
	ВыгрузитьПоПравилу(Источник.Ответственный,,,,"Пользователи");
	
	//Нети (Батрасова А. 15.09.2016, наряд 0000015576
	Если ТипЗнч(Источник) = Тип("ДокументОбъект.РасчетСтраховыхВзносов") Тогда
		ТЧОсновныеНачисления = Источник.ОсновныеНачисления.Выгрузить();
		//сворачиваем для исключения повторной выгрузки
		ТЧСотрудники = ТЧОсновныеНачисления.Скопировать();
		ТЧСотрудники.Свернуть("Сотрудник");
		ТЧФизлица = ТЧОсновныеНачисления.Скопировать();
		ТЧФизлица.Свернуть("ФизЛицо");
		//физ.лица
		Для Каждого СтрФЛ Из ТЧФизлица Цикл
			ВыгрузитьПоПравилу(СтрФЛ.ФизЛицо,,,,"ФизическиеЛица");
		КонецЦикла;
		//сотрудники
