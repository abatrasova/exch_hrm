			<Текст>// - нвВидДок

МассивВР = Новый Массив;
МассивВР.Добавить(ПланыВидовРасчета.ОсновныеНачисленияОрганизаций.РайонныйКоэффициент);
МассивВР.Добавить(ПланыВидовРасчета.ОсновныеНачисленияОрганизаций.СевернаяНадбавка);
МассивВР.Добавить(ПланыВидовРасчета.ОсновныеНачисленияОрганизаций.ОкладПоЧасам);

Выборка = Запросы.ОсновнойВидРасчетаУПР.Выполнить().Выбрать();
Если Выборка.Следующий() Тогда
	ВРДляДокумента = Выборка.ВидНачисления;
Иначе
	ВРДляДокумента = ПланыВидовРасчета.ОсновныеНачисленияОрганизаций.ОкладПоЧасам;
КонецЕсли;

Для Каждого СтрокаТЧ Из Объект.Начисления Цикл
	Если МассивВР.Найти(СтрокаТЧ.ВидРасчета) &lt;&gt; Неопределено Тогда
